<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="store.cnhk.pojo.CnhkProduct" table="tb_cnhkproduct" schema="cnhk">
        <id name="id" column="Id"/>
        <property name="serviceName" column="serviceName"/>
        <property name="servicePrice" column="servicePrice"/>
        <property name="serviceDesc" column="serviceDesc"/>
        <property name="timestamp" column="timestamp"/>
    </class>
</hibernate-mapping>